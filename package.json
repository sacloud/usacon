{
  "name": "usacon",
  "version": "0.2.2",
  "description": "A browser extension for using SakuraCloud CLI in the control panel",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/sacloud/usacon.git"
  },
  "license": "AGPL-3.0-only",
  "author": {
    "name": "The UsaCon Authors",
    "url": "https://github.com/sacloud"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@sacloud/constants": "^0.0.3",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/shell-quote": "^1.6.2",
    "@wasmer/io-devices": "^0.11.2",
    "@wasmer/wasi": "^0.11.2",
    "@wasmer/wasmfs": "^0.11.2",
    "comlink": "^4.3.0",
    "path-browserify": "^1.0.1",
    "randomfill": "^1.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "shell-parse": "^0.0.2",
    "shell-quote": "^1.7.2",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.3",
    "webpack": "^4.46.0",
    "xterm": "^4.6.0",
    "xterm-addon-fit": "^0.4.0",
    "xterm-addon-web-links": "^0.4.0"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --watch --config webpack.config.js --info-verbosity verbose",
    "build:production": "webpack --config webpack.prod.js",
    "watch:production": "webpack --watch --config webpack.prod.js --info-verbosity verbose",
    "start": "webpack-cli serve --open --config webpack.config.js",
    "start:main": "webpack-cli serve --open --config webpack.config.js",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx,js,jsx}'",
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.10.1",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/chrome": "^0.0.114",
    "babel-jest": "^26.0.1",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "jest": "^26.0.1",
    "prettier": "2.0.5",
    "react-test-renderer": "^16.13.1",
    "source-map-loader": "^1.0.0",
    "source-map-support": "^0.5.19",
    "style-loader": "^1.2.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
